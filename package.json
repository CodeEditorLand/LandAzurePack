{
	"name": "vscode-azureextensionpack",
	"displayName": "Azure Extension Pack",
	"version": "0.0.1",
	"private": false,
	"description": "The ultimate collection of extensions for working with Azure in VS Code!",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"devDependencies": {
		"typescript-esbuild": "latest"
	},
	"extensionPack": [
		"VisualStudioOnlineApplicationInsights.application-insights",
		"ms-azuretools.azurerm-vscode-tools",
		"ms-azuretools.vscode-azureappservice",
		"ms-azuretools.vscode-azurefunctions",
		"ms-azuretools.vscode-azurestorage",
		"ms-azuretools.vscode-azureterraform",
		"ms-azuretools.vscode-cosmosdb",
		"ms-azuretools.vscode-docker",
		"ms-kubernetes-tools.vscode-kubernetes-tools",
		"ms-mssql.mssql",
		"ms-toolsai.vscode-ai",
		"ms-vscode.azure-account",
		"ms-vscode.azurecli",
		"ms-vsts.team",
		"mshdinsight.azure-hdinsight",
		"usqlextpublisher.usql-vscode-ext",
		"vsciot-vscode.azure-iot-edge",
		"vsciot-vscode.azure-iot-toolkit"
	],
	"publishConfig": {
		"access": "public"
	},
	"icon": "azure-icon.png",
	"galleryBanner": {
		"color": "#0072c6",
		"theme": "dark"
	}
}
